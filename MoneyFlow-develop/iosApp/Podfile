target 'iosApp (iOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  platform :ios, '14.0'

  # Pods for iosApp (iOS)
  pod 'shared', :path => '../shared'
  pod 'SwiftyDropbox'

end

